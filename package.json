{
  "name": "react-in-screen-center",
  "version": "0.0.1",
  "description":
    "Utility components for checking if content is in the center of the viewport",
  "author": "Laurin Quast <laurinquast@googlemail.com>",
  "license": "MIT",
  "module": "./lib/index.js",
  "main": "./lib/bundle.umd.js",
  "files": ["lib", "src/index.js", "README.md", "LICENSE"],
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "build":
      "rimraf lib && babel src --no-babelrc --out-dir lib --copy-files --presets=@babel/preset-react --plugins=@babel/plugin-transform-flow-strip-types && cpy ./src/index.js ./lib --rename=index.flow.js && rollup -c",
    "lint": "eslint --ignore-pattern .gitignore **/*.js",
    "prettier-check":
      "prettier-check --ignore-path .gitignore '**/*.{js,md,json}'",
    "prettier-fix":
      "prettier --ignore-path .gitignore --write '**/*.{js,md,json}'"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.46",
    "@babel/core": "7.0.0-beta.46",
    "@babel/plugin-transform-flow-comments": "7.0.0-beta.46",
    "@babel/plugin-transform-flow-strip-types": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "cpy-cli": "1.0.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-react": "7.7.0",
    "flow-bin": "0.72.0",
    "jest": "22.4.3",
    "lint-staged": "7.1.0",
    "prettier": "1.12.1",
    "prettier-check": "2.0.0",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-babel": "4.0.0-beta.4"
  },
  "dependencies": {
    "create-react-context": "0.2.2"
  }
}
